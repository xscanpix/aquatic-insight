{
  "name": "aquatic-insight",
  "author": "Svante Nilsson",
  "repository": {
    "type": "git",
    "url": "https://github.com/svantenilsson/aquatic-insight.git"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate:create": "node --env-file=.env.local src/db/migrate create --folder src/db/migrations",
    "db:migrate:up": "node --env-file=.env.local src/db/migrate up",
    "db:migrate:down": "node --env-file=.env.local src/db/migrate down",
    "db:migrate:pending": "node --env-file=.env.local src/db/migrate pending",
    "db:migrate:executed": "node --env-file=.env.local src/db/migrate executed"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@tanstack/react-query": "^5.51.15",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "next": "^14.2.5",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "sequelize": "^6.37.3",
    "tailwind-merge": "^2.4.0",
    "zod": "^3.23.8",
    "umzug": "^3.8.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.40",
    "sequelize-cli": "^6.6.2",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "ts-node": "^10.9.2"
  }
}
